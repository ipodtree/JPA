CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE PERSON(PERSONID INTEGER NOT NULL PRIMARY KEY,NAME VARCHAR(255))
CREATE MEMORY TABLE VEHICLE(VEHICLEID INTEGER NOT NULL PRIMARY KEY,DTYPE VARCHAR(31),PLATENUMBER VARCHAR(255))
CREATE MEMORY TABLE RENT(RENTID INTEGER NOT NULL PRIMARY KEY,BEGIN DATE,DEADLINE DATE,PERSON_PERSONID INTEGER,VEHICLE_VEHICLEID INTEGER,CONSTRAINT FK_RENT_PERSON_PERSONID FOREIGN KEY(PERSON_PERSONID) REFERENCES PERSON(PERSONID),CONSTRAINT FK_RENT_VEHICLE_VEHICLEID FOREIGN KEY(VEHICLE_VEHICLEID) REFERENCES VEHICLE(VEHICLEID))
CREATE MEMORY TABLE CAR(VEHICLEID INTEGER NOT NULL PRIMARY KEY,NUMBEROFSEATS VARCHAR(255),CONSTRAINT FK_CAR_VEHICLEID FOREIGN KEY(VEHICLEID) REFERENCES VEHICLE(VEHICLEID))
CREATE MEMORY TABLE VAN(VEHICLEID INTEGER NOT NULL PRIMARY KEY,MAXWEIGHT VARCHAR(255),CONSTRAINT FK_VAN_VEHICLEID FOREIGN KEY(VEHICLEID) REFERENCES VEHICLE(VEHICLEID))
